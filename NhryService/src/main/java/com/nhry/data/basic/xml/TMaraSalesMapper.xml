<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nhry.data.basic.dao.TMaraSalesMapper" >
  <resultMap id="BaseResultMap" type="com.nhry.data.basic.domain.TMaraSalesKey" >
    <id column="mara_id" property="maraId" jdbcType="VARCHAR" />
    <id column="SALES_ORG" property="salesOrg" jdbcType="VARCHAR" />
  </resultMap>
  <delete id="deleteMaraSales" parameterType="com.nhry.data.basic.domain.TMaraSalesKey" >
    delete from t_mara_sales
    where mara_id = #{maraId,jdbcType=VARCHAR}
      and SALES_ORG = #{salesOrg,jdbcType=VARCHAR}
  </delete>
  <insert id="insertMaraSales" parameterType="com.nhry.data.basic.domain.TMaraSalesKey" >
    insert into t_mara_sales (mara_id, SALES_ORG)
    values (#{maraId,jdbcType=VARCHAR}, #{salesOrg,jdbcType=VARCHAR})
  </insert>
  <select id="isMaraSales" parameterType="com.nhry.data.basic.domain.TMaraSalesKey" resultType="java.lang.Integer">
    SELECT count(1) FROM t_mara_sales WHERE mara_id = #{maraId,jdbcType=VARCHAR}
      and SALES_ORG = #{salesOrg,jdbcType=VARCHAR}
  </select>
</mapper>